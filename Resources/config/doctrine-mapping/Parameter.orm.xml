<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
	http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
	
	<entity name="ASF\WebsiteBundle\Entity\Parameter" table="asf_website_parameter">
	    
	    <id name="id" type="integer" column="id">
	    	<generator strategy="AUTO" />
	    </id>
	    
		<field name="alias" type="string" nullable="false" />
	    
	    <field name="value" type="text" nullable="false" />
	    
		<many-to-one field="Group" target-entity="Group" inversed-by="parameters">
			<join-column name="group_id" referenced-column-name="id" />
		</many-to-one>

	</entity>
	
</doctrine-mapping>