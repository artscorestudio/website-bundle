{
	"name" : "artscorestudio/website-bundle",
	"license" : "MIT",
	"type" : "library",
	"description" : "Website Bundle is a Symfony 2/3 bundle for create and manage frontends in your Symfony 2/3 application. This package is a part of Artscore Studio Framework.",
	"homepage" : "https://github.com/artscorestudio/website-bundle",
	"authors" : [{
		"name" : "Nicolas Claverie",
		"email" : "info@artscore-studio.fr"
	}, {
		"name" : "Artscore Studio",
		"homepage" : "http://www.artscore-studio.fr"
	}
	],
	"require" : {
        "php" : ">=5.3.9",
        
        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/expression-language": "~2.8|~3.0",
        "symfony/browser-kit": "~2.8|~3.0",
        "symfony/css-selector": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/event-dispatcher": "~2.8|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/assetic-bundle": "^2.7",
        
        "doctrine/orm": "~2.4|^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "~1.0|^1.2",
        
        "twig/twig": "~1.10",
        "knplabs/knp-menu-bundle": "^2.0",
        
        "artscorestudio/core-bundle": "^1.0@dev",
        "artscorestudio/layout-bundle": "^1.0@dev",
        "artscorestudio/document-bundle": "dev-master",
        "artscorestudio/user-bundle": "dev-master"
	},
	"require-dev": {
	    "doctrine/doctrine-fixtures-bundle": "^2.3",
        "liip/functional-test-bundle": "^1.5",
        "nelmio/alice": "^2.1",
        "hautelook/alice-bundle": "^1.2",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "symfony/security": "~2.8|~3.0",
        "symfony/security-csrf": "~2.8|~3.0",
        "symfony/swiftmailer-bundle": "^2.3"
	},
	"autoload" : {
		"psr-4" : {
			"ASF\\WebsiteBundle\\" : ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.2.x-dev"
		}
	},
	"minimum-stability" : "dev"
}
